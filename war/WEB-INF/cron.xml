<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/GenerateNicolive</url>
    <description>Generate Nicolive</description>
    <schedule>every 1 hours</schedule>
    <timezone>Asia/Tokyo</timezone>
  </cron>
  <cron>
    <url>/DeleteOldNicoliveIndex</url>
    <description>Delete Old NicoliveIndex</description>
    <schedule>every day 00:00</schedule>
    <timezone>Asia/Tokyo</timezone>
  </cron>
  <cron>
    <url>/myCalendar/importAll</url>
    <description>Import Nicolive to Google Calendar</description>
    <schedule>every 12 hours</schedule>
    <timezone>Asia/Tokyo</timezone>
  </cron>
</cronentries>